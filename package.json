{
  "name": "tree-sitter-magik",
  "version": "0.0.1",
  "description": "Magik grammar for tree-sitter",
  "repository": "github:krn-robin/tree-sitter-magik",
  "license": "MIT",
  "author": "Robin Putters <robinputters@github.com>",
  "contributors": [
    "Sebastiaan Speck <sebastiaanspeck@github.com>"
  ],
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "magik"
  ],
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**"
  ],
  "dependencies": {
    "node-addon-api": "^8.1.0",
    "node-gyp-build": "^4.8.2"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.1"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "devDependencies": {
    "eslint": "^9.12.0",
    "eslint-config-treesitter": "^1.0.2",
    "tree-sitter-cli": "^0.24.2",
    "prebuildify": "^6.0.1"
  },
  "scripts": {
    "install": "node-gyp-build",
    "lint": "eslint grammar.js",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "node --test bindings/node/*_test.js",
    "prebuildify": "prebuildify --napi --strip"
  },
  "tree-sitter": [
    {
      "scope": "source.magik",
      "file-types": [
        "magik"
      ],
      "highlights": "queries/highlights.scm"
    }
  ]
}
